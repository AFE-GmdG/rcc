{
	"presets": [
		["@babel/preset-react"],
		["@babel/preset-typescript"],
		["@babel/preset-env", {
			"targets": { "esmodules": true },
			"spec": true,
			"loose": false,
			"modules": false,
			"debug": false,
			"useBuiltIns": false
		}]
	],

	"plugins": [
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread",
		"@babel/plugin-syntax-dynamic-import"
	]
}
